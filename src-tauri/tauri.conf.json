{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-simple-demo",
  "version": "0.1.0",
  "identifier": "com.tauri-simple-demo.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [],
    "trayIcon": {
      "iconPath": "icons/32x32.png"
    },
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
